{
	"name": "ibrand/notification",
	"type": "library",
	"description": "iBrand's laravel notifications package",
	"keywords": [
		"ibrand",
		"component",
		"notifications"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "shjchen",
			"email": "ibrand.shjchen@foxmail.com"
		}
	],
	"require": {
		"php": ">=7.0",
		"overtrue/wechat": "~4.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~6.0",
		"orchestra/testbench": "~3.5",
		"orchestra/database": "~3.5"
	},
	"autoload": {
		"psr-4": {
			"iBrand\\Notification\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"iBrand\\Notification\\Test\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"iBrand\\Notification\\NotificationServiceProvider"
			]
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
